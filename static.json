{
    "root": "build/",
    "routes": {
        "/**": "index.html"
    },
    "proxies": {
      "/api/": {
        "origin": "https://hanpyo-server.herokuapp.com/"
      },
      "/graphql": {
        "origin": "https://hanpyo-server.herokuapp.com/"
      }
    },
    "logging": {
        "access": false,
        "error": "debug"
    },
    "headers": {
      "/**": {
        "Access-Control-Allow-Origin": "*"
      }
    }
}
